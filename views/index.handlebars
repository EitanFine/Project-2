<div class="container">
  <div class="row"></div>
  <!-- Collect the nav links, forms, and other content for toggling -->
  <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
    <ul class="nav navbar-nav">
      {{#each categories}}
      <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Dropdown
          <span class="caret"></span>
        </a>
        <ul class="dropdown-menu">
          {{this.categoryName}}
        </ul>
      </li>
    </ul>
    <form class="navbar-form navbar-left">
      <div class="form-group">
        <input type="text" class="form-control" placeholder="Enter Zip Code">
      </div>
      <button type="submit" class="btn btn-default">Submit</button>
    </form>
    <ul class="nav navbar-nav navbar-right">
      <button type="button" class="btn btn-default navbar-btn">List Your Item</button>
    </ul>
  </div>
  <!-- /.navbar-collapse -->
  <hr>
  <div class="panel panel-primary">
    <div class="panel-heading">
      <h3 class="panel-title">{{this.categoryName}}</h3>
    </div>
    <div class="panel-body">
      <div class="row">
        {{#each Item}}
        <div class="col-sm-4 col-md-3">
          <div class="thumbnail">
            {{this.itemImage}}
            <img src="http://via.placeholder.com/350x250" alt="...">
            <div class="caption">
              <h3>{{this.itemName}}</h3>
              <p>{{this.itemDescription}}</p>
              <p>{{this.itemPrice}}</p>
              <p>
                <a href="more-info.html" class="btn btn-primary" role="button">View Listing</a>
                <form class="create-update-form" action="cocktails/create" method="POST">
                  <div class="form-group">
                    <textarea class="form-control input-lg" type="text" name="this.inuse" value="1" rows="3"> </textarea>
                  </div>
                  <button class="btn btn-default btn-lg" type="submit">Rent Iteml</button>
                </form>
              </p>
            </div>
          </div>
        </div>
        {{/each}}


      </div>
    </div>
  </div>

</div>
<!-- /.container-fluid -->
<!-- </nav> -->
</div>