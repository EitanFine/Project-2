<div class="container-fluid">

        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title">{{this.itenName}}</h3>
            </div>
            <div class="panel-body">

                <div class="row">
                    <div class="col-md-4 col-xs-4">
                        <div class="thumbnail">
                            <img src="https://domodel.net/wp-content/uploads/2014/04/gazonokosilka.jpg" alt="">
                            {{!-- {{this.itemImage}} --}}
                        </div>
                    </div>
                    <div class="col-md-4 col-xs-4">
                        <h4>DESCRIPTION</h4>
                        <p>{{this.itemDescription}}</p>
                        <br>
                        <h4>{{this.name}}</h4>
                        <br>
                        <h4>Price: ${{this.itemPrice}}</h4>
                        <br>
                        <h4>Address: {{this.streetAddress}}, {{this.city}}, {{this.state}}, {{this.zipcode}}</h4>
                        <br>
                        <h4>Contact:{{this.email}}. </h4>
                        <br>
                        <a href="#">
                            <button type="button" class="btn btn-primary dropdown-toggle">Rent It</button>
                        </a>

                    </div>

                    <div class="col-md-4 col-xs-4">

                        <!-- <div id="map"></div> -->
                        <div class="mapWrapper">
                            <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d3023.9704865667854!2d-74.0118683!3d40.7186662!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c25a1e0db46837%3A0x97328f4c4e77998f!2s199+Chambers+St%2C+New+York%2C+NY+10007!5e0!3m2!1sen!2sus!4v1414997345912" width="600" height="400" frameborder="0" style="border:0"></iframe>
                        </div> <!-- end .mapWrapper-->

                    </div>
                </div>
            </div>
        </div>
    </div>